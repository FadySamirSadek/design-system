{
  "name": "@hackclub/design-system",
  "version": "0.0.1",
  "description": "Hack Club Design System",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run icons && babel src -d dist --ignore __tests__",
    "precommit": "npm run fmt",
    "dev": "start-storybook -p 8000 -c .storybook",
    "build": "rm -rf .site && build-storybook -c .storybook -o .site",
    "start": "serve .site -p 9001",
    "icons": "node scripts/build-icons",
    "fmt": "prettier {.storybook,scripts,src,dist}/**/*.js --single-quote --no-semi --write",
    "coverage": "jest --coverage && serve coverage -o",
    "test": "jest"
  },
  "author": "Lachlan Campbell",
  "license": "MIT",
  "dependencies": {
    "@storybook/addon-info": "v3.3.0-rc.0",
    "@storybook/react": "v3.3.0-rc.0",
    "dot-prop": "^4.2.0",
    "palx": "^1.0.2",
    "prop-types": "^15.6.0",
    "styled-components": "^2.4.0",
    "styled-system": "^1.1.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "lint-staged": "^6.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "serve": "^6.4.3",
    "stylis": "^3.4.5",
    "svgi": "^1.1.0"
  },
  "devDependencies": {
    "@storybook/storybook-deployer": "^2.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme": "^3.2.0",
    "material-design-icons": "^3.0.1",
    "prettier": "^1.9.2",
    "jest": "^22.0.4",
    "jest-styled-components": "^4.9.0",
    "react-test-renderer": "^16.2.0"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "dist/"
    ],
    "setupTestFrameworkScriptFile": "./test-setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hackclub/design-system.git"
  },
  "bugs": {
    "url": "https://github.com/hackclub/design-system/issues"
  },
  "homepage": "https://github.com/hackclub/design-system#readme"
}
